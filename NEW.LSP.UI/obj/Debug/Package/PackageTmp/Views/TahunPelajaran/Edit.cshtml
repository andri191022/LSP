@model NEW.LSP.UI.Models.m_Tb_Tahun_Pelajaran
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div class="card card-primary">
        <div class="card-header">
            <h3 class="card-title">Edit</h3>
        </div>
        <div class="card-body">

            <div class="form-horizontal">

                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group">
                    @Html.LabelFor(model => model.Tahun_pelajaran, htmlAttributes: new { @class = "control-label col-md-6" })
                    <div class="col-md-10">
                        @Html.EditorFor(model => model.Tahun_pelajaran, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.Tahun_pelajaran, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="card-footer">
                    @Html.ActionLink("Back to List", "Index", new { area = "" }, new { @class = "btn btn-success" })
                    <input type="submit" value="Save" class="btn btn-primary float-right" />
                    @section Scripts {
                        @Scripts.Render("~/bundles/jqueryval")
                    }
                </div>
            </div>
        </div>
    </div>

}

<script>
