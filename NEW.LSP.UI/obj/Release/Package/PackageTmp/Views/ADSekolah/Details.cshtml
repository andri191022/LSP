@model NEW.LSP.UI.Models.m_Tb_Admin_Sekolah_cstm
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>
    <div class="card card-primary">
        <div class="card-header">
            <h3 class="card-title">Details</h3>
        </div>

        <div class="card-body">
            <div class="row">
                <div class="col-2" style="font-weight: bold;">
                    @Html.DisplayNameFor(model => model.Username)
                    <hr />
                </div>
                <div class="col-1 text-center">
                    :
                </div>
                <div class="col-9">
                    @Html.DisplayFor(model => model.Username)
                    <hr />
                </div>
            </div>

            <div class="row">
                <div class="col-2" style="font-weight: bold;">
                    @Html.DisplayNameFor(model => model.Password)
                    <hr />
                </div>
                <div class="col-1 text-center">
                    :
                </div>
                <div class="col-9">
                    ********
                    <hr />
                </div>
            </div>

            <div class="row">
                <div class="col-2" style="font-weight: bold;">
                    @Html.DisplayNameFor(model => model.NPSN)
                    <hr />
                </div>
                <div class="col-1 text-center">
                    :
                </div>
                <div class="col-9">
                    @Html.DisplayFor(model => model.NPSN) &nbsp; - &nbsp; @Html.DisplayFor(model => model.Nama_Sekolah)
                    <hr />
                </div>
            </div>

        </div>

        <div class="card-footer">
            @Html.ActionLink("BACK", "Index", "ADSekolah", new { area = "" }, new { @class = "btn btn-primary" })
            <div class=" float-right">
                <b>Creator:</b> @Html.DisplayFor(model => model.creator) &nbsp;-&nbsp; @if (@Model.created != null)
                {@Model.created.Value.ToString("dd/MMM/yyyy HH:mm")},&nbsp;<b>Editor:</b> @Html.DisplayFor(model => model.editor)&nbsp;-&nbsp; @if (@Model.edited != null)
                {@Model.edited.Value.ToString("dd/MMM/yyyy HH:mm")}
            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $("#linkBar").html('<ol class="breadcrumb float-sm-right"><li class="breadcrumb-item"><a href="/Home">Home</a></li><li class="breadcrumb-item"><a href="../">Data Admin Sekolah</a></li><li class="breadcrumb-item active">Detail Data</li></ol >');
        $("#dashbordMain").html("Detail Data");

        ADMenuPropinsi('mnuAdminSekolah');
    });
</script>

