                        @Html.EditorFor(model => model.Nama_KK, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.Nama_KK, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>

            <div class="card-footer">
                @Html.ActionLink("Back to List", "Index", new { area = "" }, new { @class = "btn btn-success" })
